import { chatgpt4 } from "../../lib/ai.js";
export default async ({ sock, m, id, psn, sender, noTel, caption }) => {
    if (psn === '') {
        sock.sendMessage(id, {
            text: "prefix *ai3* Tanyakan sesuatu kepada AI 3\n contoh : ai3 siapa presiden indonesia saat ini"
        })
        return
    }
    try {
        await sock.sendMessage(id, { text: await chatgpt4(psn) });
    } catch (error) {
        await sock.sendMessage(id, { text: `Terjadi kesalahan di sisi server ${error}` });
    }
};
